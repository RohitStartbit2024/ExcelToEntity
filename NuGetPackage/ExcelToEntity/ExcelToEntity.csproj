<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

	  <!-- NuGet Info -->
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageId>ExcelToEntity</PackageId>
	<Version>1.0.1</Version>
	<Authors>RohitAgarwal</Authors>
	<Company>StartbitItSolutions</Company>
	<Description>A simple Excel to Entity importer using ClosedXML</Description>
	<PackageTags>Excel Import ClosedXML</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ClosedXML" Version="0.105.0" />
  </ItemGroup>

</Project>
